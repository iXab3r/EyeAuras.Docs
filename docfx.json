{
  "metadata": [
    {
      "src": [
        {
          "src": "../../Sources/EyeAuras.SDK/bin",
          "files": [
            "BlazorRete.dll",
            "PropertyBinder.dll",
            "PoeShared*.dll",
            "EyeAuras*.Metadata.dll",
            "EyeAuras.*Auras.dll",
            "EyeAuras.Roxy*.dll",
            "EyeAuras.BehaviorTree.dll",
            "EyeAuras.Blazor*.dll",
            "EyeAuras.Shared*.dll",
            "EyeAuras.Graphics*.dll",
            "EyeAuras.Scripting.dll",
          ],
          "exclude": [
            "**/DirectML.Debug.dll",
            "**/DirectML.dll",
            "**/onnxruntime.dll",
            "**/EyeAuras.Web*.dll",
            "**/EyeAuras.Kings*.dll",
          ]
        }
      ],
      "dest": "api",
      "filter": "api-filter.yml",
      "memberLayout": "separatePages",
      "namespaceLayout": "nested",
      "disableGitFeatures": true,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "globalMetadata": {
      "_appTitle": "EyeAuras Docs",
      "_appName": "EyeAuras Docs",
      "_appLogoPath": "images/logo.png",
      "_appFaviconPath": "images/favicon.png",
      "_copyrightFooter": "Â© Xab3r. All rights reserved.",
      "_enableSearch": true,
      "_enableNewTab": true,
      "pdf": true,
    },
    "template": ["default", "templates/singulinkfx"],
    "xrefService": [ "https://xref.docs.microsoft.com/query?uid={uid}" ],
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "docs/**"
        ]
      }
    ],
    "dest": "docs",
    "disableGitFeatures": true,
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false
  }
}